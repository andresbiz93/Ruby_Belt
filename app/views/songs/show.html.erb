<a href = "/songs">Dashboard</a>

<h3>Others who added <%= @song.artist %> - <%= @song.title %></h3>

<% if @entries.length > 0 %>

    <table class = "table">

        <thead class = "thead-dark">

            <tr>

                <th>Name</th>

                <th>Times Added</th>

            </tr>

        </thead>

        <tbody>

            <% @entries.each do |entry| %>

                <tr>

                    <td><a href = "/users/<%= entry.user.id %>"><%= entry.user.first_name %> <%= entry.user.last_name %></a></td>

                    <td><%= entry.number %></td>

                </tr>

            <% end %>

        </tbody>

    </table>

<% end %>
