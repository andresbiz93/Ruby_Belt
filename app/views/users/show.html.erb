<a href = "/songs">Dashboard</a>

<h3><%= @user.first_name %> <%= @user.last_name %>'s playlist</h3> 

<table class = "table">

    <thead class = "thead-dark">

        <tr>

            <th>Artist</th>

            <th>Title</th>

            <th>Times Added</th>

        </tr>

    </thead>

    <tbody>

        <% @entries.each do |entry| %>

            <tr>

                <td><%= entry.song.artist %></td>
                <td><a href = "/songs/<%= entry.song.id %>"><%= entry.song.title %></a></td>
                <td><%= entry.number %></td>

            </tr>

        <% end %>

    </tbody>

</table>

